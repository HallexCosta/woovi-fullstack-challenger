{
  "name": "@woovi/bank-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "tsx --watch ./src/server.ts",
    "copy:env": "cp .env.example .env",
    "test": "vitest",
    "seeds": "CLI=1 tsx ./src/seeds.ts",
    "test:coverage": "vitest --coverage",
    "build:tsc": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "rm -rf dist && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" --ignore \"*.spec.ts,**/*.d.ts\" --out-dir dist --copy-files --verbose",
    "build-schema": "mkdir -p schema && CLI=1 tsx scripts/buildSchema.ts",
    "docs:generate:collections": "mkdir -p docs && gql2postman -s ./schema/schema.graphql -o ./docs/graphql-api-collection.json -p -O depth=3,includeDeprecatedFields=true"
  },
  "dependencies": {
    "@woovi/server-utils": "workspace:^",
    "axios": "^1.7.7",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^8.2.0",
    "graphql": "16.8.1",
    "graphql-http": "^1.22.1",
    "graphql-playground-middleware-koa": "^1.6.22",
    "graphql-relay": "^0.10.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-graphql": "^0.12.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^12.0.1",
    "module-alias": "^2.2.3",
    "mongoose": "^7.0.3",
    "secure-random": "^1.1.2",
    "vitest": "^2.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@koa/cors": "^5.0.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv-safe": "^8.1.6",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.13.12",
    "@types/koa-cors": "^0.0.6",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-router": "^7.4.8",
    "@types/koa__cors": "^5.0.0",
    "@types/node": "^17.0.12",
    "@types/secure-random": "^1.1.3",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-istanbul": "^2.1.1",
    "@woovi/babel": "workspace:*",
    "@woovi/tsconfig": "workspace:*",
    "mongodb-memory-server": "^10.0.0",
    "supertest": "^7.0.0",
    "supertest-graphql": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "tsx": "^3.12.6",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
